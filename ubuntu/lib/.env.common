# Common env variables shared between shells
# Contains same variables as FILEVARS.sh

# BEGIN Already Set
export EDITOR=$(command -v vim)
export PAGER=$(command -v less)
export UBUNTU_DOTFILES=~/.dotfiles/ubuntu
# END Already Set

INITVIM=~/.config/nvim/init.vim
INITVIM_GIT="$UBUNTU_DOTFILES/init.vim"


MY_BASH_CONFIG=(~/.bashrc ~/.profile ~/.shell_functions)
MY_BASH_CONFIG+=~/.bash_aliases

